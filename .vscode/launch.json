{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "name": "Launch index.html",
      "file": "${workspaceFolder}/App.tsx"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 3500,
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/webservice",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}